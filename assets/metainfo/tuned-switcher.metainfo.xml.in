<component type="desktop">
    <id>@SCHEME_NAME@</id>
    <metadata_license>CC0-1.0</metadata_license>
    <project_license>GPL-3.0-or-later</project_license>
    <name>Tuned Switcher</name>
    <summary>@CMAKE_PROJECT_DESCRIPTION@</summary>
    <description>
        <p>
            Tuned Switcher is a simple utility to manipulate the Tuned service.
        </p>
        <p>
            Tuned is a daemon for monitoring and adaptive tuning of system devices.
            In order to use this program, this daemon must be installed on your system.
        </p>
    </description>
    <screenshots>
        <screenshot type="default">
            <image>https://www.easycoding.org/files/images/tuned-switcher.png</image>
        </screenshot>
    </screenshots>
    <url type="homepage">@CMAKE_PROJECT_HOMEPAGE_URL@</url>
    <url type="bugtracker">@CMAKE_PROJECT_HOMEPAGE_URL@/issues</url>
    <update_contact>@CMAKE_PROJECT_HOMEPAGE_URL@</update_contact>
    <launchable type="desktop-id">@APPLET_LAUNCHER@</launchable>
    <content_rating type="oars-1.0" />
    <releases>
        <release version="@CMAKE_PROJECT_VERSION@" date="@RELEASE_DATE@" />
    </releases>
</component>
